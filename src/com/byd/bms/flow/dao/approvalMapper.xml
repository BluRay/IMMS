<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.byd.bms.flow.dao.IApprovalDao">
    <!-- 添加审核记录信息 -->
	<insert id="save" useGeneratedKeys="true" parameterType="com.byd.bms.flow.model.Approval">
		<selectKey resultType="int" keyProperty="id" order="AFTER">
			SELECT
			LAST_INSERT_ID()
		</selectKey>
		<![CDATA[
			insert into BMS_FLOW_APPROVAL(operator,operate_time,result,description,order_id,task_id,task_name,content) 
			values(#{operator},#{operateTime},#{result},#{description},#{orderId},#{taskId},#{taskName},#{content})
		]]>
	</insert>
</mapper>
